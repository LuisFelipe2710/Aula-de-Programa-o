EXEMPLO EM C#

using System; 

namespace HelloWorld 
{ 
  class helloWorld 
  { 
    static void Main(string[] args) 
    { 
      Console.WriteLine("Meu nome é Luis Felipe, e eu quero um 10"); 
      Console.ReadKey(); 
    }
   } 
} (editado)


